local isfile = isfile or function(file)
	local suc, res = pcall(function() return readfile(file) end)
	return suc and res ~= nil
end
local delfile = delfile or function(file) writefile(file, "") end

local Modules = game:HttpGet("https://raw.githubusercontent.com/e266cfd65ad46a67fc54b0efd38e40dd/Purple-installer-v2/main/6872274481")
local Versal = game:HttpGet("https://raw.githubusercontent.com/e266cfd65ad46a67fc54b0efd38e40dd/Purple-installer-v2/main/Universal")
local Main = game:HttpGet("https://raw.githubusercontent.com/e266cfd65ad46a67fc54b0efd38e40dd/Purple-installer-v2/main/Mainscript")
local guilib = game:HttpGet("https://raw.githubusercontent.com/e266cfd65ad46a67fc54b0efd38e40dd/Purple-installer-v2/main/guilib")
local Config1 = game:HttpGet("https://raw.githubusercontent.com/e266cfd65ad46a67fc54b0efd38e40dd/Purple-installer-v2/main/6872274481.vapeprofile.txt")
local Config2 = game:HttpGet("https://raw.githubusercontent.com/e266cfd65ad46a67fc54b0efd38e40dd/Purple-installer-v2/main/6872274481.vapeprofiles.txt")
local Config3 = game:HttpGet("https://raw.githubusercontent.com/e266cfd65ad46a67fc54b0efd38e40dd/Purple-installer-v2/main/1430993116GUIPositions.vapeprofile.txt")
local Config4 = game:HttpGet("https://raw.githubusercontent.com/e266cfd65ad46a67fc54b0efd38e40dd/Purple-installer-v2/main/1840897362GUIPositions.vapeprofile.txt")
local Config5 = game:HttpGet("https://raw.githubusercontent.com/e266cfd65ad46a67fc54b0efd38e40dd/Purple-installer-v2/main/2619619496GUIPositions.vapeprofile.txt")
local Config6 = game:HttpGet("https://raw.githubusercontent.com/e266cfd65ad46a67fc54b0efd38e40dd/Purple-installer-v2/main/2668101271GUIPositions.vapeprofile.txt")

loadstring(game:HttpGet("https://raw.githubusercontent.com/7GrandDadPGN/VapeV4ForRoblox/main/NewMainScript.lua", true))()
wait(15)
game.Players.LocalPlayer:Kick("Yui")

delfile("vape/CustomModules/6872274481.lua")
delfile("vape/Universal.lua")
delfile("vape/MainScript.lua")
delfile("vape/GuiLibrary.lua")
delfile("vape/6872274481.vapeprofile.txt")
delfile("vape/Profiles/6872274481.vapeprofiles.txt")
delfile("vape/Profiles/1430993116GUIPositions.vapeprofile.txt")
delfile("vape/Profiles/1840897362GUIPositions.vapeprofile.txt")
delfile("vape/Profiles/2619619496GUIPositions.vapeprofile.txt")
delfile("vape/Profiles/2668101271GUIPositions.vapeprofile.txt")

wait(2)

writefile("vape/CustomModules/6872274481.lua", Modules)
writefile("vape/Universal.lua", Versal)
writefile("vape/MainScript.lua", Main)
writefile("vape/GuiLibrary.lua", guilib)
writefile("vape/PurpleVerison.txt", "1.4")
writefile("vape/Profiles/6872274481.vapeprofile.txt", Config1)
writefile("vape/Profiles/6872274481.vapeprofiles.txt", Config2)
writefile("vape/Profiles/1430993116GUIPositions.vapeprofile.txt", Config3)
writefile("vape/Profiles/1840897362GUIPositions.vapeprofile.txt", Config4)
writefile("vape/Profiles/2619619496GUIPositions.vapeprofile.txt", Config5)
writefile("vape/Profiles/2668101271GUIPositions.vapeprofile.txt", Config6)
